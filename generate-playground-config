#!/bin/bash
IFS=''
echo "# !!!!!!!!!!!!!!!!!! Autogenerated using make !!!!!!!!!!!!!!!!!!"
while read line; do
    if [[ "${line:0:13}" == ">>build-plan " ]]; then
        for i in ${line:13}; do
            heading=`head -n1 $i`;
            echo "  - title: ${heading:2}";
            echo "    statement: $i";
        done
    >&2 echo "built plan from ${line:13}"
    else
        echo "$line";
    fi;
done < techio.yml.in
